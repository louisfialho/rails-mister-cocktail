<ul class="cards">
  <% @cocktails.each do |cocktail| %>
    <li>

      <div class="card" style="width: 18rem;">
        <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"> <%= link_to cocktail.name, cocktail_path(cocktail) %> </h5>
          <p class="card-text">
            <ul>
              <% cocktail.doses.each do |dose| %>
                <li>
                  <%=  dose.ingredient.name %>
                </li>
              <% end %>
            </ul>
          </p>
        </div>
      </div>


    </li>
  <% end %>
</ul>

 <%= link_to "Create a cocktail", cocktails_new_path, class: 'btn btn-primary' %>

